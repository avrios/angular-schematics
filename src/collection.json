{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "api-service": {
      "description": "Create an Avrios API service",
      "factory": "./api-service",
      "schema": "./api-service/schema.json"
    },
    "component": {
      "description": "Create an Avrios component",
      "factory": "./component",
      "schema": "./component/schema.json"
    },
    "modal": {
      "description": "Create an Avrios modal",
      "factory": "./modal",
      "schema": "./modal/schema.json"
    },
    "page-component": {
      "description": "Create an Avrios page component",
      "factory": "./page-component",
      "schema": "./page-component/schema.json"
    },
    "store": {
      "description": "Create an Avrios ngrx store",
      "factory": "./store",
      "schema": "./store/schema.json"
    },
    "library": {
      "description": "Create an Avrios library",
      "factory": "./library",
      "schema": "./library/schema.json"
    }
  }
}
